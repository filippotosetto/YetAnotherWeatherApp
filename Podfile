# Uncomment this line to define a global platform for your project

use_frameworks!

target 'YetAnotherWeatherApp' do
	platform :ios, '9.0'
    pod "SwiftOpenWeatherMapAPI"#, :path => '../SwiftOpenWeatherMapAPI' 
    pod 'LMGeocoder'
end

target 'Watch Extension' do
	platform :watchos, '2.0'
    pod "SwiftOpenWeatherMapAPI"#, :path => '../SwiftOpenWeatherMapAPI'
end


